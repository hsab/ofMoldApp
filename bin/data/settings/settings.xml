<mycelia>
  <framerate>60</framerate>
  <osdColor>
    <r>255</r>
    <g>255</g>
    <b>255</b>
  </osdColor>
  <canvas>
    <width>1280</width>
    <height>720</height>
    <shaderFilename>shaders/alphamask.frag</shaderFilename>
    <maskFilename>images/bread_profile_mask.png</maskFilename>
      <backgroundColor>
      <r>0</r>
      <g>10</g>
      <b>0</b>
    </backgroundColor>
  </canvas>
  <mycelium>
    <lifespan>99999</lifespan>
    <conidia>
      <active>0</active>
      <conidium>
        <lifespan>2000</lifespan>
        <delay>10</delay>
        <recenterRatio>0.98</recenterRatio>
      </conidium>
    </conidia>
    <hyphae>
      <active>1</active>
      <color>
        <r>255</r>
        <g>255</g>
        <b>255</b>
        <a>10</a>
      </color>
      <creationAreaSize>5</creationAreaSize>
      <primalHyphaCount>20</primalHyphaCount>
      <newHyphaPeriod>20</newHyphaPeriod>
      <maxHyphaCount>10000</maxHyphaCount>
      <hypha>
        <maxLifespan>100000</maxLifespan>
        <speed>0.01</speed>
        <speedVariation>10</speedVariation> <!--%-->
        <gravity>0.0001</gravity>
        <maxHeight>100</maxHeight>
        <distortion>1.0</distortion>
        <forkAgeRatio>2</forkAgeRatio>
        <maxForkAngle>15</maxForkAngle>
        <maxBendAngle>0.1</maxBendAngle>
        <fertilityRateRatio>3</fertilityRateRatio>
        <fertilityRatePower>3</fertilityRatePower>
      </hypha>
    </hyphae>
  </mycelium>
</mycelia>